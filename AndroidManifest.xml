<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="net.newriverclimbing.vous"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".Home"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name=".ScheduleListActivity"></activity>
        <activity android:name=".ScheduleDetailActivity"></activity>
        <activity android:name=".SponsorListActivity"></activity>
        <activity android:name=".SponsorDetailActivity"></activity>
        <activity android:name=".WeatherActivity"></activity>
        <activity android:name=".WeatherDailyActivity"></activity>
        <activity android:name=".WeatherHourlyActivity"></activity>
        <activity android:name=".WeatherAboutActivity"></activity>
        <activity android:name=".MapViewActivity"></activity>
        <activity android:name=".MapDetailActivity"></activity>
        <activity android:name=".MapOverviewActivity"></activity>
        <activity android:name=".RegistrationActivity"></activity>
        <activity android:name=".SupportActivity"></activity>
        
        <uses-library android:name="com.google.android.maps"/>
        
    </application>
    <uses-sdk android:minSdkVersion="4" />
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

</manifest> 